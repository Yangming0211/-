<!DOCTYPE html>
<html>
	<head>
		<meta charset="utf-8">
		<title></title>
	</head>
	<body>
		<script>
			i=0;
			count=0;
			while(i<100){
				if(i%3==0 || i%2==0){
					i++;
					continue;
				}
				count++;
				if(count>8){
					document.write("<br>");
					count=0;
				}
				document.write("&nbsp",i);
				i++;
			}
		</script>
	</body>
</html>
